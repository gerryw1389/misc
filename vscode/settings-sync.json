{
   "[markdown]": {
      "files.trimTrailingWhitespace": false,
      "editor.tabSize": 3
   },
   "[python]": {
      "editor.renderWhitespace": "boundary",
      "editor.tabSize": 4,
      "editor.rulers": [
         79,
         120
      ],
      "editor.defaultFormatter": "ms-python.python"
   },
   "breadcrumbs.enabled": false,
   "cSpell.enabledLanguageIds": [
      "asciidoc",
      "cpp",
      "latex",
      "markdown",
      "plaintext",
      "text"
   ],
   "azureFunctions.projectSubpath": "dev",
   "azureFunctions.showProjectWarning": false,
   "diffEditor.ignoreTrimWhitespace": false,
   "editor.acceptSuggestionOnEnter": "off",
   "editor.autoClosingBrackets": "never",
   "editor.codeLens": false,
   "editor.cursorStyle": "line",
   "editor.detectIndentation": false,
   "editor.foldingStrategy": "indentation",
   "editor.renderControlCharacters": false,
   "editor.tabSize": 3,
   "[terraform]": {
      "editor.defaultFormatter": "hashicorp.terraform",
      "editor.formatOnSave": true
   },
   "editor.tokenColorCustomizations": {
      "comments": "#9E9E9E",
      "textMateRules": [
         {
            "scope": "meta.paragraph.markdown",
            "settings": {
               "foreground": "#9E9E9E"
            }
         },
         {
            "scope": [
               "markup.inline.raw",
               "markup.fenced_code.block.markdown"
            ],
            "settings": {
               "foreground": "#9AA83A"
            }
         },
         {
            "scope": [
               "markup.heading.markdown",
               "entity.name.tag",
               "entity.name.section.markdown",
               "meta.link.inline.markdown",
               "markup.underline",
               "markup.bold",
               "markup.italic",
               "markup.list",
               "meta.separator.markdown",
               "meta.link.reference.def.markdown",
               "punctuation.definition.heading.markdown",
               "heading.3.markdown",
               "heading.2.markdown",
               "heading.1.markdown"
            ],
            "settings": {
               "foreground": "#228B22"
            }
         },
         {
            "scope": [
               "markup.raw.block.markdown",
               "constant.character.escape.markdown",
               "markup.inserted.math.inline.markdown"
            ],
            "settings": {
               "foreground": "#FF69B4"
            }
         }
      ],
      "[Relax Dark]": {
         "textMateRules": [
            {
               "name": "comment.line.number-sign.python",
               "scope": [
                  "comment.line.number-sign.python",
                  "punctuation.definition.comment.python"
               ],
               "settings": {
                  "foreground": "#228B22"
               }
            },
            {
               "name": "source.python",
               "scope": "source.python",
               "settings": {
                  "foreground": "#9E9E9E"
               }
            },
            {
               "name": "meta.item-access.arguments.python",
               "scope": "meta.item-access.arguments.python",
               "settings": {
                  "foreground": "#FF69B4"
               }
            }
         ]
      }
   },
   "editor.wordWrap": "on",
   "explorer.autoReveal": false,
   "explorer.compactFolders": false,
   "explorer.confirmDelete": false,
   "explorer.confirmDragAndDrop": false,
   "extensions.ignoreRecommendations": true,
   "fileheader.Author": "Gerry Williams",
   "fileheader.LastModifiedBy": "Gerry Williams",
   "fileheader.tpl": "---\n@Author: {author}\n@Date Created: {createTime}\n---\n\n",
   "files.autoSave": "afterDelay",
   "files.autoSaveDelay": 2000,
   "files.eol": "\n",
   "files.hotExit": "off",
   "git.autofetch": true,
   "git.confirmSync": false,
   "git.decorations.enabled": true,
   "git.enableSmartCommit": true,
   "git.fetchOnPull": true,
   "git.path": "C:\\Program Files\\Git\\bin\\git.exe",
   "insertDateString.format": "YYYY-MM-DDThh:mm:ssZZZ",
   "insertDateString.formatDate": "YYYY-MM-DD",
   "powershell.codeFormatting.autoCorrectAliases": true,
   "powershell.codeFormatting.preset": "Allman",
   "python.analysis.logLevel": "Warning",
   "python.formatting.provider": "black",
   "python.linting.pylintEnabled": true,
   "python.linting.enabled": true,
   "python.pythonPath": "C:\\Program Files\\Python39\\python.exe",
   "terminal.integrated.cursorStyle": "underline",
   "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
   "typescript.tsserver.watchOptions": {
      "watchDirectory": "dynamicPriorityPolling",
      "watchFile": "dynamicPriorityPolling"
   },
   "update.mode": "manual",
   "vsicons.dontShowNewVersionMessage": true,
   "window.newWindowDimensions": "maximized",
   "window.restoreWindows": "none",
   "window.title": "${rootName}",
   "workbench.iconTheme": "vscode-icons",
   "workbench.sideBar.location": "left",
   "workbench.startupEditor": "newUntitledFile",
   "workbench.tree.renderIndentGuides": "always",
   "workbench.colorTheme": "Relax Dark",
   "workbench.colorCustomizations": {
      "editorCursor.foreground": "#228B22",
      "terminalCursor.foreground": "#228B22"
   },
   "workbench.editorAssociations": [
      {
         "viewType": "jupyter-notebook",
         "filenamePattern": "*.ipynb"
      }
   ],
   "redhat.telemetry.enabled": false
}